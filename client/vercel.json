{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/print/(.*)",
      "dest": "https://your-render-backend-url/api/v1/print/$1",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)", 
      "dest": "/dist/$1",
      "continue": true
    }
  ]
}